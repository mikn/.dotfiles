Host *
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p

Host github.com
    User git
    IdentityFile ~/.ssh/id_github

Host asr*
    User admin
    KexAlgorithms +diffie-hellman-group1-sha1
    HostkeyAlgorithms ssh-rsa

Host sw-*
    User admin
    KexAlgorithms +diffie-hellman-group1-sha1
    HostkeyAlgorithms ssh-dss
